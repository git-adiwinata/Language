{
    "name": "akunting/language",
    "description": "Language switcher package for Laravel.",
    "keywords": ["laravel", "language", "switcher"],
    "license": "MIT",
    "authors": [{
        "name": "Bursamaya",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=6.0",
        "consoletvs/identify": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Akunting\\Language\\": "./src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akunting\\Language\\Provider"
            ],
            "aliases": {
                "Language": "Akunting\\Language\\Facade"
            }
        }
    }
}
